{
  "name": "commutable",
  "version": "1.1.3",
  "description": "Immutable notebook model and operations",
  "main": "lib/index.js",
  "scripts": {
    "repl": "babel-node scripts/repl.js",
    "prepublish": "npm run build",
    "prebuild": "mkdirp lib && npm run clean",
    "build": "npm run build:ts",
    "build:ts": "typings install && tsc -p ./src",
    "build:es5": "babel src --out-dir lib/ --source-maps",
    "build:docs": "jsdoc -R README.md -d docs lib/*.js",
    "lint": "eslint src",
    "test": "npm run build && mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "posttest": "npm run lint",
    "clean": "rimraf lib/*",
    "precoverage": "nyc npm run test",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nteract/commutable.git"
  },
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nteract/commutable/issues"
  },
  "homepage": "https://github.com/nteract/commutable#readme",
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash.repeat": "^4.0.0",
    "uuid": "^3.0.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "chalk": "^1.1.1",
    "codecov": "^2.0.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jsdoc": "^3.4.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "notebook-test-data": "1.0.1",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.0",
    "typescript": "^2.1.5",
    "typings": "^2.1.0"
  }
}
